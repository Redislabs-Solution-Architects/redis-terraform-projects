# =============================================================================
# REDIS CLOUD SIMPLE - CONFIGURATION
# Copy this file to terraform.tfvars and fill in your values
# =============================================================================

# Redis Cloud API Credentials
# Get these from: https://app.redislabs.com -> Access Management -> API Keys
rediscloud_api_key    = "your-account-api-key-here"
rediscloud_secret_key = "your-user-secret-key-here"

# AWS Configuration
aws_account_id = "123456789012"  # Your 12-digit AWS Account ID

# Payment Information
credit_card_last_four = "1234"  # Last 4 digits of your credit card
